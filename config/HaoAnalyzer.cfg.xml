<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>HAO Analyzer 扩展配置</comment>
    <!-- 算法模型路径 -->
    <entry key="baseDictionary">base_dictionary.txt</entry>
    <entry key="customerDictionaryFile">custom_dictionary.txt</entry>
    <!-- 第一次执行的时间 HH:MM:ss 建议每天23点同步一次,如果为空则当前时间300s后开始执行-->
    <entry key="syncDicTime">23:00:00</entry>
    <!-- <entry key="syncDicTime"></entry> -->
    <!-- 执行周期 -->
    <entry key="syncDicPeriodTime">86400</entry>
    <!-- <entry key="syncDicPeriodTime">3600</entry> -->
    <!-- <entry key="remoteFreqDict"></entry> -->
    <entry key="remoteFreqDict">http://api.ersinfotech.com/helper-api/dic</entry>
    <!-- <entry key="remoteFreqDict">http://172.31.38.74:3000/dic</entry> -->
    <!-- 钉钉机器人URL -->
    <entry key="dingWebHookUrl">
        https://oapi.dingtalk.com/robot/send?access_token=d7f1
    </entry>
    <!-- 钉钉通知文案 -->
    <entry key="dingMsgContent">本地分词器失败，失败内容:{text}，失败原因:{reason}</entry>

</properties>
